{
	"root": true,
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"es6": true
	},
	"rules": {
		"no-console": 0
	},
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"requireConfigFile": false,
		"ecmaVersion": 5,
		"sourceType": "module",
		"allowImportExportEverywhere": true
	}
}
