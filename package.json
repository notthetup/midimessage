{
  "name": "midimessage",
  "version": "1.1.0",
  "description": "Web-MIDI Message Parser",
  "main": "dist/index.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "browserify demo.js -t babelify -o demobundle.js && http-server",
    "test-es5": "browserify demo-es5.js -o demobundle.js && http-server",
    "build": "eslint src/*.js && browserify src/index.js --standalone midimessage -t babelify -o dist/index.js && uglifyjs dist/index.js --mangle --source-map --output dist/index.min.js"
  },
  "browserify": {
    "transform": [["babelify", { "presets": ["@babel/preset-env"] }]]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notthetup/midimessage.git"
  },
  "keywords": [
    "midi",
    "parse",
    "message"
  ],
  "author": "Chinmay Pendharkar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notthetup/midimessage/issues"
  },
  "homepage": "https://github.com/notthetup/midimessage#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "npm-check-updates": "^12.5.2",
    "uglify-js": "^3.15.2",
    "eslint": "^8.10.0"
  }
}
